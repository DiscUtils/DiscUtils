{
  "version": "0.12.1-alpha",

  "title": "DiscUtils",
  "description": "DiscUtils, complete meta-package",
  "authors": [ "Kenneth Bell", "LordMike" ],

  "packOptions": {
    "owners": [ "LordMike", "qmfrederik" ],
    "tags": [ "DiscUtils" ],
    "projectUrl": "https://github.com/DiscUtils/DiscUtils",
    "licenseUrl": "https://github.com/DiscUtils/DiscUtils/blob/master/LICENSE.txt",
    "repository": {
      "type": "git",
      "url": "https://github.com/DiscUtils/DiscUtils"
    }
  },

  "dependencies": {
    "DiscUtils.Core": "0.12.1-alpha",
    "DiscUtils.BootConfig": "0.12.1-alpha",
    "DiscUtils.Dmg": "0.12.1-alpha",
	"DiscUtils.Ewf": "0.12.1-alpha",
    "DiscUtils.Ext": "0.12.1-alpha",
    "DiscUtils.Fat": "0.12.1-alpha",
    "DiscUtils.HfsPlus": "0.12.1-alpha",
    "DiscUtils.Iscsi": "0.12.1-alpha",
    "DiscUtils.Iso9660": "0.12.1-alpha",
    "DiscUtils.Nfs": "0.12.1-alpha",
    "DiscUtils.Ntfs": "0.12.1-alpha",
    "DiscUtils.OpticalDisk": "0.12.1-alpha",
    "DiscUtils.Registry": "0.12.1-alpha",
    "DiscUtils.Sdi": "0.12.1-alpha",
    "DiscUtils.SquashFs": "0.12.1-alpha",
    "DiscUtils.Udf": "0.12.1-alpha",
    "DiscUtils.Vdi": "0.12.1-alpha",
    "DiscUtils.Vhd": "0.12.1-alpha",
    "DiscUtils.Vhdx": "0.12.1-alpha",
    "DiscUtils.Vmdk": "0.12.1-alpha",
    "DiscUtils.Wim": "0.12.1-alpha",
    "DiscUtils.Xva": "0.12.1-alpha"
  },

  "buildOptions": {
    "publicSign": true,
    "keyFile": "../../SigningKey.snk"
  },

  "frameworks": {
    "netstandard1.5": {
      "buildOptions": {
        "define": [ "NETCORE" ]
      }
    },
    "net20": {
      "dependencies": {
        "DiscUtils.Net": "0.12.1-alpha",
        "DiscUtils.OpticalDiscSharing": "0.12.1-alpha"
      },
      "buildOptions": {
        "define": [ "NET20" ]
      }
    },
    "net40": {
      "dependencies": {
        "DiscUtils.Net": "0.12.1-alpha",
        "DiscUtils.OpticalDiscSharing": "0.12.1-alpha"
      }
    },
    "net45": {
      "dependencies": {
        "DiscUtils.Net": "0.12.1-alpha",
        "DiscUtils.OpticalDiscSharing": "0.12.1-alpha"
      }
    }
  }
}