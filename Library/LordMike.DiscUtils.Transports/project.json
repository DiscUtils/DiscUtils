    },
    "net40": {
      "dependencies": {
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    },
    "net45": {
      "dependencies": {
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    }
  }
