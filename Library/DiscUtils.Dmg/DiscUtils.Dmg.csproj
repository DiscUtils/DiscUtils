<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>DiscUtils dmg parser. Works with apple disk (.dmg) files</Description>
    <Authors>Kenneth Bell;LordMike;quamotion</Authors>
    <PackageTags>DiscUtils;dmg</PackageTags>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <TargetFrameworks>netstandard2.0;net462;net6.0</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\DiscUtils.Core\DiscUtils.Core.csproj" />
    <PackageReference Include="lzfse-net" Version="1.0.15" />
  </ItemGroup>

</Project>
